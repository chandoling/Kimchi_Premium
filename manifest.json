{
  "manifest_version": 3,
  "name": "김치 프리미엄 위젯",
  "version": "1.1",
  "description": "김치 프리미엄을 툴바에 표시합니다.",
  "action": {
    "default_icon": "icon/kimchi_copyright_Freepik.png"
  },
  "icons": {
    "16": "icon/kimchi_copyright_Freepik.png",
    "48": "icon/kimchi_copyright_Freepik.png",
    "128": "icon/kimchi_copyright_Freepik.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "alarms"
  ],
  "host_permissions": [
    "https://api.exchangerate-api.com/*",
    "https://api.binance.com/*",
    "https://api.upbit.com/*"
  ]
}
